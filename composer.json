{
    "name": "remp/beam-skeleton",
    "description": "REMP Beam Skeleton",
    "type": "project",
    "homepage": "https://remp2030.com",
    "license": ["MIT"],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "snowplow/referer-parser-php",
                "version": "0.0.1",
                "source": {
                    "url": "https://github.com/snowplow-referer-parser/php-referer-parser",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "Snowplow\\": "src/Snowplow"
                    }
                }
            }
        }
    ],
    "require": {
        "remp/beam-module": "@dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan vendor:publish --tag=beam-assets"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
